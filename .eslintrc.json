{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google", "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "linebreak-style": "off",
      "import/prefer-default-export": "off",
      "require-jsdoc" : "off",
      "new-cap"  : "off",
      "max-len": "off"
    }
}
